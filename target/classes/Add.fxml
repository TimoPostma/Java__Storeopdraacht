<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.timo.store.AddController">
   <children>
      <VBox layoutX="20.0" layoutY="14.0" prefHeight="368.0" prefWidth="560.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Brand" />
            <TextField fx:id="brand" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Type" />
            <TextField fx:id="type" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Price" />
            <TextField fx:id="price" />
            <Button fx:id="submit" mnemonicParsing="false" onAction="#submit" text="Voeg Toe" />
         </children>
      </VBox>
   </children>
</AnchorPane>
